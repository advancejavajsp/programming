package number;

public class NthFibonacci {
    public static void main(String[] args) {
        int n = 7;
        System.out.println(n + "th Fibonacci Number: " + fib(n));
    }

    static int fib(int n) {
        if (n <= 1)
            return n;
        return fib(n - 1) + fib(n - 2);
    }
}

