package array;

public class BinarySearch {
public static void main(String[] args) {
	int[] a=	{ 1,2,3,4,5,6,8,12,14,23,52 };
	int k=52;
	int start=0;
	int end=a.length-1;
	int pos=-1;
	while(start<end) {
		int mid=(start+end)/2;
		if(a[mid]==k) {
			pos=-1;
			break;
		}
		else if(a[mid]>k) {
			start=mid+1;
			
		}
		else if(a[mid]<k)
			end=mid-1;
	}
	System.out.println("element present at index "+pos);
}
}
